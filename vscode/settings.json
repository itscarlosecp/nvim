	// GENERAL SETTINGS
	"editor.fontFamily": "Cascadia Code",
	"editor.fontLigatures": false,
	"editor.fontSize": 14,
	"files.defaultLanguage": "python",
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.startupPrompt": "never",
	"security.workspace.trust.untrustedFiles": "open",
	"terminal.integrated.fontFamily": "CaskaydiaCove NF",
	"workbench.iconTheme": "material-icon-theme",

	// EDITOR (BEHAVIOUR)
	"editor.detectIndentation": true,
	"editor.formatOnSave": true,
	"editor.insertSpaces": false,
	"editor.lineNumbers": "relative",
	"editor.tabCompletion": "on",
	"editor.tabSize": 4,
	"editor.wordWrap": "on",

	// EDITOR (INTERFACE)
	"editor.cursorSurroundingLines": 5, // Scrolloff
	"editor.minimap.enabled": false,
	"editor.rename.enablePreview": false,
	"workbench.editor.enablePreview": false,
	"workbench.editor.limit.enabled": true,
	"workbench.editor.limit.perEditorGroup": true,
	"workbench.editor.limit.value": 2,
	"workbench.editor.showTabs": false,
	"workbench.list.automaticKeyboardNavigation": false, // Disable explorer filtering

	// FILE EXPLORER
	"explorer.openEditors.visible": 0,
	"explorer.compactFolders": false,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"*.out": true,
		"*.o": true
	},

	// VIM
	"vim.autoindent": true,
	"vim.easymotion": true,
	"vim.foldfix": true,
	"vim.hlsearch": true,
	"vim.incsearch": true,
	"vim.leader": "<space>",
	"vim.useCtrlKeys": true,
	"vim.useSystemClipboard": true,
	"vim.visualstar": true,
	"vim.normalModeKeyBindingsNonRecursive": [
		// GLOBALS
		{
			"before": ["<leader>", "x"],
			"commands": ["workbench.action.showCommands"]
		},
		// FILE EXPLORER & FUZZY FINDER
		{
			"before": ["<leader>", "e"],
			"commands": ["workbench.view.explorer"]
		},
		{
			"before": ["<leader>", "f", "f"],
			"commands": ["workbench.action.quickOpen"]
		},
		// SEARCH & REPLACE
		{
			"before": ["/"],
			"commands": ["actions.find"]
		},
		{
			"before": ["?"],
			"commands": ["workbench.action.findInFiles"]
		},
		// DIAGNOSTICS
		{
			"before": ["[", "g"],
			"commands": ["editor.action.marker.next"]
		},
		{
			"before": ["]", "g"],
			"commands": ["editor.action.marker.prev"]
		}
	],
	"vim.visualModeKeyBindingsNonRecursive": [
		// SEARCH & REPLACE
		{
			"before": ["/"],
			"commands": ["actions.find"]
		},
		{
			"before": ["?"],
			"commands": ["workbench.action.findInFiles"]
		}
	],

	// LANGUAGE SPECIFICS
	"emmet.includeLanguages": { "javascript": "javascriptreact" },
	"html.format.wrapLineLength": 80,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"python.condaPath": "/home/itscarlosecp/anaconda3/bin/python",
	"python.defaultInterpreterPath": "/usr/bin/python3",
	"python.pythonPath": "/usr/bin/python3",
	"python.showStartPage": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"[css]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[go]": { "editor.tabSize": 4 },
	"[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[javascriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[python]": {
		"editor.insertSpaces": true,
		"editor.defaultFormatter": "ms-python.python",
		"editor.tabSize": 4
	},
	"[scss]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },

	// PRETTIER
	"prettier.jsxSingleQuote": false,
	"prettier.semi": false,
	"prettier.singleQuote": false,
	"prettier.tabWidth": 2,
	"prettier.useTabs": true,

	// OTHERS
	"terminal.integrated.defaultProfile.linux": "bash",
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	}
}
